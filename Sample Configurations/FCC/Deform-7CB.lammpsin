angle_coeff     22 harmonic   45.411090    80.0
angle_coeff     23 harmonic   45.411090   180.0

group           centr type 1
group           solvent molecule > 12000
group           metal1 molecule 1 
group           metal2 molecule 2000 
group           metal3 molecule 4000 
group           metal4 molecule 6000

group           rigid type 4 5 6 7 8 9
group           prerd type 4 5 8 6 9 
group           sideoxy type 8
group           sideoxy include molecule
group           triox intersect prerd sideoxy

group           lcbiphe type 9
group           lcbiphe include molecule
group           lcyst intersect lcbiphe prerd

group           lgbiphe type 6
group           lgbiphe include molecule
group           biphe intersect lgbiphe prerd

group           bprgd intersect biphe rigid
group           trrgd intersect triox rigid
group           lcrgd intersect lcyst rigid
group           other subtract all trrgd metal1 metal2 metal3 metal4 
group           union union trrgd metal1 metal2 metal3 metal4 
group           sol type 9

delete_bonds    union multi
#delete_atoms    random fraction 0.15 no sol NULL 87227 bond yes mol yes

compute         pressure all pressure thermo_temp
fix             rigid union rigid/nve/small molecule
fix             com1 metal1 spring tether 1000  0.0 0.0 0.0 0

thermo          1000
thermo_style    custom step cpuremain temp pe etotal zlo xlo ylo press

timestep        5.0
fix             deform all deform 1 z vel 1e-7 x volume y volume 
restart         5000 solRunZD4-A.restart solRunZD4-B.restart
fix             tempControl1 other nvt temp 300.0 300.0 100 dilate all
write_data      Deform-17nm.data

dump            dumpCalc0 all atom 10000 tran_deform-sample-A.lammpstrj
run             20000000 
undump          dumpCalc0
write_data      Deform-18nm.data

dump            dumpCalc1 all atom 10000 tran_deform-sample-B.lammpstrj
run             20000000 
undump          dumpCalc1
write_data      Deform-19nm.data

dump            dumpCalc2 all atom 10000 tran_deform-sample-C.lammpstrj
run             20000000 
undump          dumpCalc2
write_data      Deform-20nm.data






